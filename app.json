{
  "expo": {
    "name": "anydle",
    "slug": "anydle",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/branding/logo.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/branding/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "anydle",
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/branding/logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.daamin.anydle"
    },
    "web": {
      "favicon": "./assets/branding/logo.png"
    },
    "plugins": [
      "expo-router"
    ],
    "ios": {
      "bundleIdentifier": "com.daamin.anydle"
    }
  }
}
